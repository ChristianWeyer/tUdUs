@* Automatically grabs the installed version of JQuery/JQueryUI/KnockoutJS *@
@foreach (var item in Directory.EnumerateFiles(Server.MapPath(@"~/Scripts"), "jquery-?.?.?.js"))
{
  <script src="~/Scripts/@Path.GetFileName(item)"></script>
